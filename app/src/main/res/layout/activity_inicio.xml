        android:layout_marginEnd="336dp"
        android:layout_marginBottom="582dp"
        android:text="Apps"
        android:textSize="25dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
